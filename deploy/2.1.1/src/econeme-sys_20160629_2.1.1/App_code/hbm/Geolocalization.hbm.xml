<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="com.nemesys.model" assembly="App_Code">
    <class name="Geolocalization" table="GOOGLEMAP_LOCALIZATION">
		<id name="id">
		    <column name="id" sql-type="int(11)" not-null="true"/>
		    <generator class="native" />
		</id>	
  	
		<property name="idElement" column="id_element" not-null="true" />
		<property name="type" column="type" not-null="true" />
		<property name="latitude" column="latitude" type="Decimal" />
		<property name="longitude" column="longitude" type="Decimal" />
		<property name="txtInfo" column="txtinfo" />	
    </class>
</hibernate-mapping>