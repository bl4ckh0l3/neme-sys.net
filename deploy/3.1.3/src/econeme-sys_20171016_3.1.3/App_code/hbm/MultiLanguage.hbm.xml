<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="com.nemesys.model" assembly="App_Code">
    <class name="MultiLanguage" table="MULTI_LANGUAGES">
        <id name="id">
            <column name="id" sql-type="int(20)" not-null="true"/>
            <generator class="native" />
        </id>
		<property name="keyword" column="keyword" not-null="true" />
		<property name="langCode" column="lang_code" not-null="true" />
		<property name="value" column="value" />
    </class>
</hibernate-mapping>